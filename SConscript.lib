Import('env')

lib_src = ['gpu_solver.c']
lib_objs = env.SharedObject(env['COMMON_SRC'] +
                                env['KERNEL_GEN'](env) +
                                lib_src)
shared_lib = env.SharedLibrary('silentarmy', lib_objs)

# install the shared library into the python package
env.Install('#pysa', shared_lib)
env.Alias('pyinstall', '#pysa')
